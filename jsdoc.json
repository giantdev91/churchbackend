{
  "opts": {
    "template": "node_modules/better-docs"
  },
  "tags": {
    "allowUnknownTags": ["component"]
  },
  "source": {
    "include": [ "./src" ],
    "exclude": [ "node_modules" ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["jsdoc-http-plugin"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
